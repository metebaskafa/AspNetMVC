@{
    List<string> kategoriler = new List<string>()
    {
        "bilgisayar", "elektronik", "telefon"
    };
}
<ul class="list-group">
    @foreach (var item in kategoriler)
    {
        <li class ="list-group-item">
            <a href="/Home?kategori=@item">
                @item
            </a>
        </li>
        
    }
</ul>